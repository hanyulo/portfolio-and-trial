{
  "name": "portfolio-and-trial",
  "version": "1.0.9",
  "description": "url shortener front end project",
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.4.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "pm2": "^4.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
    "react-router-dom": "^5.1.2",
    "isomorphic-style-loader": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/react": "^9.3.2",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.11.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "isomorphic-style-loader/withStyles": "<rootDir>/tools/__mocks__/withStyles.js",
      "automock": "true"
    }
  },
  "scripts": {
    "test:snapshot": "jest --updateSnapshot",
    "test": "jest",
    "build:client": "webpack --config webpack.config.production.js",
    "build:server": "webpack --config webpack.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "gcloud:deploy": "gcloud app deploy --project portfolio-and-trial --version 1-0-0",
    "gcloud:browse": "gcloud app browse",
    "start:dev": "NODE_ENV=development PORT=3000 node webpack-dev-server.js",
    "start:prod": "npm run build && node ./dist/server.js",
    "start": "node ./dist/server.js",
    "pm2:restart": "node_modules/.bin/pm2 kill --silent",
    "start:prod:pm2": "npm run pm2:restart; npm run build && node_modules/.bin/pm2 start ecosystem.config.js",
    "start:pm2": "npm run pm2:restart; node_modules/.bin/pm2 start ecosystem.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanyulo/url-shortener-front-end.git"
  },
  "keywords": [
    "url",
    "shortener",
    "express",
    "node"
  ],
  "author": "Han",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanyulo/url-shortener-front-end/issues"
  },
  "homepage": "https://github.com/hanyulo/url-shortener-front-end#readme"
}
